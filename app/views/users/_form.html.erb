<%= form_with model: @user do |form| %>
    <div>
      <%= form.label :first_name %><br>
      <%= form.text_field :title %>
      <% @user.errors.full_messages_for(:title).each do |message| %>
        <div><%= message %></div>
      <% end %>
    </div>
  
    <div>
      <%= form.label :last_name %><br>
      <%= form.text_field :last_name %><br>
      <% @user.errors.full_messages_for(:content).each do |message| %>
        <div><%= message %></div>
      <% end %>
    </div>
    <div>
    <%= form.label :email %><br>
    <%= form.text_field :email %>
    <% @user.errors.full_messages_for(:title).each do |message| %>
      <div><%= message %></div>
    <% end %>
  </div>

  <div>
    <%= form.label :password %><br>
    <%= form.text_field :password %><br>
    <% @user.errors.full_messages_for(:content).each do |message| %>
      <div><%= message %></div>
    <% end %>
  </div>

  <div>
    <%= form.label :reenter_password %><br>
    <%= form.text_field :reenter_password %><br>
    <% @user.errors.full_messages_for(:content).each do |message| %>
      <div><%= message %></div>
    <% end %>
  </div>
    <div>
      <%= form.submit %>
    </div>
  <% end %>